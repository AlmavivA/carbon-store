<div id='{{this.name}}-controls'>

</div>
        {{dump .}}
<table id='{{this.name}}' class='table fm-managed' data-use-plugins="UnboundTablePlugin"
       data-columns="{{this.columnNames}}">

    <thead>
    {{#each this.columnLabels}}
    <th>
        {{this}}
    </th>

    {{/each}}

    <th></th>
    </thead>
    <tbody>

    {{#each this.fields}}
    <tr>
        {{#each this}}
        <td>
            <!--The field should be rendered here -->
             {{> fields this}}
        </td>
        {{/each}}

        <td>
            <!--The delete button will be populated here-->
            Action!
        </td>
    </tr>
    {{/each}}
    </tbody>
</table>

