<a data-toggle="collapse" href="#collapse{{label}}" aria-expanded="false" class="collapsing-h2" >
    <h2>
                <span class="fa-stack fa-lg collapse-stack-icons">
                    <i class="fa fa-caret-right fa-stack-1x"></i>
                    <i class="fa fa-circle-thin fa-stack-2x"></i>
                </span>
        {{label}}
    </h2>
</a>
<div class="collapse in" id="collapse{{label}}">
    {{#eachField this.fields}}
            <div class="row padding-bottom-lg">
                <div class="col-lg-2 text-right">{{label}}</div>
                <div class="col-lg-10">{{value}}</div>
            </div>
    {{/eachField}}
</div>